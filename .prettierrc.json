{
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 180,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "singleAttributePerLine": false,
    "importOrder": [
        "^(react|next?/?([a-zA-Z/]*))$",
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}